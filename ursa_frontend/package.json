{
  "name": "ursa-frontend",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm run dev-menu",
    "test-colony": "vite ./colony_root --config=vite.config.ts --base=/colony --mode test",
    "test-menu": "vite ./main_menu_root --config=vite.config.ts --base=/menu --mode test",
    "test-tutorial": "vite ./tutorial_root --config=vite.config.ts --base=/tutorial --mode test",
    "dev-colony": "vite ./colony_root --config=vite.config.ts --base=/colony --mode development",
    "dev-menu": "vite ./main_menu_root --config=vite.config.ts --base=/menu --mode development",
    "dev-tutorial": "vite ./tutorial_root --config=vite.config.ts --base=/tutorial --mode development",
    "build-colony": "vite build --config=vite.config.ts ./colony_root --outDir=\"../../dist/colony\" --emptyOutDir --mode production",
    "build-menu": "vite build --config=vite.config.ts ./main_menu_root --outDir=\"../../dist/main_menu\" --emptyOutDir --mode production",
    "build-tutorial": "vite build --config=vite.config.ts ./tutorial_root --outDir=\"../../dist/tutorial\" --emptyOutDir --mode production",
    "build-all": "npm run build-colony && npm run build-menu && npm run build-tutorial"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "solid-devtools": "^0.30.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.3",
    "solid-js": "^1.8.11"
  }
}
