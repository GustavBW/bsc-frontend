{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev-colony": "vite ./colony_root --config=vite.config.ts --base=/colony",
    "dev-menu": "vite ./main_menu_root --config=vite.config.ts --base=/menu",
    "dev-tutorial": "vite ./tutorial_root --config=vite.config.ts --base=/tutorial",
    "build-colony": "vite build --config=vite.config.ts ./colony_root --outDir=\"../../dist/colony\" --emptyOutDir",
    "build-menu": "vite build --config=vite.config.ts ./main_menu_root --outDir=\"../../dist/main_menu\" --emptyOutDir",
    "build-tutorial": "vite build --config=vite.config.ts ./tutorial_root --outDir=\"../../dist/tutorial\" --emptyOutDir",
    "build-all": "npm run build-colony && npm run build-menu && npm run build-tutorial"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "solid-devtools": "^0.29.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.13.3",
    "solid-js": "^1.8.11"
  }
}
